msgctxt "STRINGS.ELEMENTS.BRICK.NAME"
msgid "<link=\"BRICK\">Brick</link>"
msgstr "<link=\"BRICK\">Кирпич</link>"

#. STRINGS.ELEMENTS.BRINE.DESC
msgctxt "STRINGS.ELEMENTS.BRINE.DESC"
msgid "Brine is a natural, highly concentrated solution of <link=\"SALT\">Salt</link> dissolved in <link=\"WATER\">Water</link>.\n\nIt can be used in desalination processes, separating out useable salt."
msgstr "Рапа — естественный высококонцентрированный <link=\"SALT\">соляной</link> раствор, в основе которого лежит <link=\"WATER\">вода</link>.\n\nПозволяет получить полезную соль при деминерализации."

#. STRINGS.ELEMENTS.BRINE.NAME
msgctxt "STRINGS.ELEMENTS.BRINE.NAME"
msgid "<link=\"BRINE\">Brine</link>"
msgstr "<link=\"BRINE\">Рапа</link>"

#. STRINGS.ELEMENTS.BRINEICE.DESC
msgctxt "STRINGS.ELEMENTS.BRINEICE.DESC"
