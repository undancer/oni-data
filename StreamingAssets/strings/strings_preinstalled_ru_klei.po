msgctxt "STRINGS.ELEMENTS.ALUMINUMORE.NAME"
msgid "<link=\"ALUMINUMORE\">Aluminum Ore</link>"
msgstr "<link=\"ALUMINUMORE\">Алюминиевая руда</link>"

#. STRINGS.ELEMENTS.BITUMEN.DESC
msgctxt "STRINGS.ELEMENTS.BITUMEN.DESC"
