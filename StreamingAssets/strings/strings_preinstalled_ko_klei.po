msgctxt "STRINGS.UI.BUILDINGEFFECTS.TOOLTIPS.MESS_TABLE_SALT"
msgid "Duplicants gain <b>+{0}</b> <style=\"KKeyword\">Morale</style> when using <style=\"KKeyword\">Table Salt</style> with their food at a <link=\"DININGTABLE\">Mess Table</link>"
